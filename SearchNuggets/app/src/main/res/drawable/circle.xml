<?xml version="1.0" encoding="utf-8"?>
<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="-90"
    android:pivotX="50%"
    android:pivotY="50%"
    android:toDegrees="270">



    <shape android:shape="ring"
        android:useLevel="true">


        <gradient
            android:type="sweep"
            android:centerY="0.5"
            android:endColor="@color/quantum_amber100"
            android:centerColor="@color/quantum_amber200"
            android:startColor="@color/quantum_lightblue500"/>
            android:useLevel="true"


    </shape>






</rotate>